<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.imooc.dmp.mapper.mysql.MysqlCrowdsMapper">

    <select id="getCrowdsByPage" resultType="com.imooc.dmp.entity.mysql.MysqlCrowds">
        select
        *
        from crowds limit #{page},#{size}
    </select>

</mapper>